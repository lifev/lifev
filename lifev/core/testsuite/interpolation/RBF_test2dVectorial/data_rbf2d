[fluid]
    [./space_discretization]
       	mesh_dir                 = ./
       	mesh_file                = fluid2d_rbf.msh
       	mesh_type                = .msh

[solid]
    [./space_discretization]
       	mesh_dir                 = ./
       	mesh_file                = solid2d_rbf.msh
       	mesh_type                = .msh

[interpolation]
    interpolation_Type           = RBFvectorial # RBFlocallyRescaledVectorial, RBFrescaledVectorial, RBFvectorial

[prec]
    prectype                     = Ifpack # Ifpack or ML
    displayList                  = false

    [./ifpack]
        overlap                  = 2

        [./fact]
            ilut_level-of-fill   = 1
            drop_tolerance       = 0.0
            relax_value          = 0

        [../amesos]
            solvertype           = Amesos_Umfpack # Amesos_KLU or Amesos_Umfpack

        [../partitioner]
            overlap              = 2

        [../schwarz]
            reordering_type      = none #metis, rcm, none
            filter_singletons    = true

		[../]
	[../]


